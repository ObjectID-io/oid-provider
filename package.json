{
  "name": "@objectid-io/oid-provider",
  "version": "1.0.5",
  "description": "Client-side ObjectID Provider SDK: executes Move calls directly (no backend).",
  "license": "MIT",
  "type": "module",
  "main": "./dist-cjs/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist-cjs/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist-cjs/react/index.js"
    }
  },
  "files": [
    "dist",
    "dist-cjs",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "node scripts/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "@iota/identity-wasm": ">=1.7.0-beta.1 <2.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@iota/iota-sdk": "^1.0.0",
    "axios": "^1.13.2",
    "open-location-code-typescript": "^1.5.0"
  },
  "devDependencies": {
    "@iota/identity-wasm": "^1.8.0-beta.2",
    "@types/react": "^18.3.5",
    "react": "^18.3.1",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ObjectID-io/oid-provider.git"
  }
}
