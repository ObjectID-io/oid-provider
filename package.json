{
  "name": "@objectid/objectid-provider",
  "version": "0.4.0",
  "description": "Client-side ObjectID Provider SDK: executes Move calls directly (no backend).",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@iota/iota-sdk": "^1.0.0"
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "react": "^18.3.1",
    "@types/react": "^18.3.5"
  }
}