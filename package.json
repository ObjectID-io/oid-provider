{
  "name": "@objectid/oid-provider",
  "version": "1.0.10",
  "description": "Client-side ObjectID Provider SDK: executes Move calls directly (no backend).",
  "license": "MIT",
  "type": "module",
  "main": "./dist-cjs/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist-cjs/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist-cjs/react/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist-cjs/core.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js",
      "require": "./dist-cjs/api.js"
    },
    "./methods": {
      "types": "./dist/methods.d.ts",
      "import": "./dist/methods.js",
      "require": "./dist-cjs/methods.js"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "import": "./dist/env.js",
      "require": "./dist-cjs/env.js"
    },
    "./oid": {
      "types": "./dist/oid/index.d.ts",
      "import": "./dist/oid/index.js",
      "require": "./dist-cjs/oid/index.js"
    }
  },
  "files": [
    "README.md",
    "api",
    "core",
    "dist",
    "dist-cjs",
    "env",
    "methods",
    "oid"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "node scripts/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "peerDependencies": {
    "@iota/identity-wasm": "^1.8.0-beta.2",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@iota/iota-sdk": "^1.0.0",
    "axios": "^1.13.2",
    "open-location-code-typescript": "^1.5.0"
  },
  "devDependencies": {
    "@iota/identity-wasm": "^1.8.0-beta.2",
    "@types/react": "^18.3.5",
    "react": "^18.3.1",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ObjectID-io/oid-provider.git"
  },
  "typesVersions": {
    "*": {
      "core": [
        "dist/core.d.ts"
      ],
      "api": [
        "dist/api.d.ts"
      ],
      "methods": [
        "dist/methods.d.ts"
      ],
      "env": [
        "dist/env.d.ts"
      ],
      "oid": [
        "dist/oid/index.d.ts"
      ]
    }
  }
}
